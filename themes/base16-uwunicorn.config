! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2f2a3f, #2f2a3f, #241b26

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2f2a3f, #eed5d9, #2f2a3f, #2f2a3f, #e4ccd0
rofi.color-active:  #2f2a3f, #6a9eb5, #2f2a3f, #2f2a3f, #6a9eb5
rofi.color-urgent:  #2f2a3f, #877bb6, #2f2a3f, #2f2a3f, #877bb6

! Set the desired separator style
rofi.separator-style: solid
