! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1a1e01, #1a1e01, #101600

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1a1e01, #ffda51, #1a1e01, #1a1e01, #ffeba0
rofi.color-active:  #1a1e01, #5b4a9f, #1a1e01, #1a1e01, #5b4a9f
rofi.color-urgent:  #1a1e01, #ee2e00, #1a1e01, #1a1e01, #ee2e00

! Set the desired separator style
rofi.separator-style: solid
