! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #ededed, #ededed, #ffffff

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #ededed, #555555, #ededed, #ededed, #555555
rofi.color-active:  #ededed, #186daa, #ededed, #ededed, #186daa
rofi.color-urgent:  #ededed, #af4947, #ededed, #ededed, #af4947

! Set the desired separator style
rofi.separator-style: solid
