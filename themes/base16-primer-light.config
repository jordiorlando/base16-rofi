! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e1e4e8, #e1e4e8, #fafbfc

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e1e4e8, #2f363d, #e1e4e8, #e1e4e8, #1b1f23
rofi.color-active:  #e1e4e8, #0366d6, #e1e4e8, #e1e4e8, #0366d6
rofi.color-urgent:  #e1e4e8, #d73a49, #e1e4e8, #e1e4e8, #d73a49

! Set the desired separator style
rofi.separator-style: solid
