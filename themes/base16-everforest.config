! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #343f44, #343f44, #2d353b

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #343f44, #d3c6aa, #343f44, #343f44, #fdf6e3
rofi.color-active:  #343f44, #7fbbb3, #343f44, #343f44, #7fbbb3
rofi.color-urgent:  #343f44, #e67e80, #343f44, #343f44, #e67e80

! Set the desired separator style
rofi.separator-style: solid
