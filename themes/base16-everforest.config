! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #374247, #374247, #2f383e

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #374247, #d3c6aa, #374247, #374247, #fdf6e3
rofi.color-active:  #374247, #a7c080, #374247, #374247, #a7c080
rofi.color-urgent:  #374247, #7fbbb3, #374247, #374247, #7fbbb3

! Set the desired separator style
rofi.separator-style: solid
