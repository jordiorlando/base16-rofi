! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #21262d, #21262d, #010409

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #21262d, #b1bac4, #21262d, #21262d, #f0f6fc
rofi.color-active:  #21262d, #58a6ff, #21262d, #21262d, #58a6ff
rofi.color-urgent:  #21262d, #ff7b72, #21262d, #21262d, #ff7b72

! Set the desired separator style
rofi.separator-style: solid
