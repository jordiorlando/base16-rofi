! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #ecf6f2, #ecf6f2, #fcfefd

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #ecf6f2, #5a605d, #ecf6f2, #ecf6f2, #2d302f
rofi.color-active:  #ecf6f2, #b0a4e3, #ecf6f2, #ecf6f2, #b0a4e3
rofi.color-urgent:  #ecf6f2, #f9906f, #ecf6f2, #ecf6f2, #f9906f

! Set the desired separator style
rofi.separator-style: solid
