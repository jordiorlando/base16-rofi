! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f3f4f5, #f3f4f5, #fafafa

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f3f4f5, #5c6773, #f3f4f5, #f3f4f5, #1a1f29
rofi.color-active:  #f3f4f5, #36a3d9, #f3f4f5, #f3f4f5, #36a3d9
rofi.color-urgent:  #f3f4f5, #f07178, #f3f4f5, #f3f4f5, #f07178

! Set the desired separator style
rofi.separator-style: solid
