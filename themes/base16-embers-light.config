! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #aeb6be, #aeb6be, #d1d6db

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #aeb6be, #323b43, #aeb6be, #aeb6be, #0f1316
rofi.color-active:  #aeb6be, #82576d, #aeb6be, #aeb6be, #82576d
rofi.color-urgent:  #aeb6be, #576d82, #aeb6be, #aeb6be, #576d82

! Set the desired separator style
rofi.separator-style: solid
