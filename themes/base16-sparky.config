! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #00313c, #00313c, #072b31

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #00313c, #f4f5f0, #00313c, #00313c, #ffffff
rofi.color-active:  #00313c, #4698cb, #00313c, #00313c, #4698cb
rofi.color-urgent:  #00313c, #ff585d, #00313c, #00313c, #ff585d

! Set the desired separator style
rofi.separator-style: solid
