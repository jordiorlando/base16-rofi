! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #303030, #303030, #222222

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #303030, #c0c0c0, #303030, #303030, #ffffff
rofi.color-active:  #303030, #00a3f2, #303030, #303030, #00a3f2
rofi.color-urgent:  #303030, #e15d67, #303030, #303030, #e15d67

! Set the desired separator style
rofi.separator-style: solid
