! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #ece3cc, #ece3cc, #fbf3db

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #ece3cc, #53676d, #ece3cc, #ece3cc, #3a4d53
rofi.color-active:  #ece3cc, #006dce, #ece3cc, #ece3cc, #006dce
rofi.color-urgent:  #ece3cc, #cc1729, #ece3cc, #ece3cc, #cc1729

! Set the desired separator style
rofi.separator-style: solid
