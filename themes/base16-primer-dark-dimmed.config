! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #373e47, #373e47, #1c2128

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #373e47, #909dab, #373e47, #373e47, #cdd9e5
rofi.color-active:  #373e47, #539bf5, #373e47, #373e47, #539bf5
rofi.color-urgent:  #373e47, #f47067, #373e47, #373e47, #f47067

! Set the desired separator style
rofi.separator-style: solid
