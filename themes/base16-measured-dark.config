! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #003a38, #003a38, #00211f

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #003a38, #dcdcdc, #003a38, #003a38, #f5f5f5
rofi.color-active:  #003a38, #88b0da, #003a38, #003a38, #88b0da
rofi.color-urgent:  #003a38, #ce7e8e, #003a38, #003a38, #ce7e8e

! Set the desired separator style
rofi.separator-style: solid
