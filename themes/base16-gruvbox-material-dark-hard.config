! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2a2827, #2a2827, #202020

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2a2827, #ddc7a1, #2a2827, #2a2827, #fbf1c7
rofi.color-active:  #2a2827, #7daea3, #2a2827, #2a2827, #7daea3
rofi.color-urgent:  #2a2827, #ea6962, #2a2827, #2a2827, #ea6962

! Set the desired separator style
rofi.separator-style: solid
