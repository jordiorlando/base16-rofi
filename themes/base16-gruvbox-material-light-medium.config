! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f2e5bc, #f2e5bc, #fbf1c7

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f2e5bc, #654735, #f2e5bc, #f2e5bc, #282828
rofi.color-active:  #f2e5bc, #45707a, #f2e5bc, #f2e5bc, #45707a
rofi.color-urgent:  #f2e5bc, #c14a4a, #f2e5bc, #f2e5bc, #c14a4a

! Set the desired separator style
rofi.separator-style: solid
