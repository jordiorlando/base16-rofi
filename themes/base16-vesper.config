! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #232323, #232323, #101010

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #232323, #b7b7b7, #232323, #232323, #d5d5d5
rofi.color-active:  #232323, #8eaaaa, #232323, #232323, #8eaaaa
rofi.color-urgent:  #232323, #de6e6e, #232323, #232323, #de6e6e

! Set the desired separator style
rofi.separator-style: solid
