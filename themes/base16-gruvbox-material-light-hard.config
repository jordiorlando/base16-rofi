! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #fbf1c7, #fbf1c7, #f9f5d7

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #fbf1c7, #654735, #fbf1c7, #fbf1c7, #282828
rofi.color-active:  #fbf1c7, #45707a, #fbf1c7, #fbf1c7, #45707a
rofi.color-urgent:  #fbf1c7, #c14a4a, #fbf1c7, #fbf1c7, #c14a4a

! Set the desired separator style
rofi.separator-style: solid
