! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #100323, #100323, #271c3a

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #100323, #dedcdf, #100323, #100323, #bbaadd
rofi.color-active:  #100323, #8e7dc6, #100323, #100323, #8e7dc6
rofi.color-urgent:  #100323, #a92258, #100323, #100323, #a92258

! Set the desired separator style
rofi.separator-style: solid
