! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #3c3836, #3c3836, #32302f

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #3c3836, #ddc7a1, #3c3836, #3c3836, #fbf1c7
rofi.color-active:  #3c3836, #7daea3, #3c3836, #3c3836, #7daea3
rofi.color-urgent:  #3c3836, #ea6962, #3c3836, #3c3836, #ea6962

! Set the desired separator style
rofi.separator-style: solid
