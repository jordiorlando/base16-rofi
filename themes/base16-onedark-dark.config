! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1c1f24, #1c1f24, #000000

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1c1f24, #abb2bf, #1c1f24, #1c1f24, #c8ccd4
rofi.color-active:  #1c1f24, #61afef, #1c1f24, #1c1f24, #61afef
rofi.color-urgent:  #1c1f24, #ef596f, #1c1f24, #1c1f24, #ef596f

! Set the desired separator style
rofi.separator-style: solid
