! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1a1b2a, #1a1b2a, #11121d

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1a1b2a, #a0a8cd, #1a1b2a, #1a1b2a, #a0a8cd
rofi.color-active:  #1a1b2a, #7199ee, #1a1b2a, #1a1b2a, #7199ee
rofi.color-urgent:  #1a1b2a, #ee6d85, #1a1b2a, #1a1b2a, #ee6d85

! Set the desired separator style
rofi.separator-style: solid
