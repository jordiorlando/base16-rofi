! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #252525, #252525, #181818

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #252525, #b9b9b9, #252525, #252525, #dedede
rofi.color-active:  #252525, #368aeb, #252525, #252525, #368aeb
rofi.color-urgent:  #252525, #ed4a46, #252525, #252525, #ed4a46

! Set the desired separator style
rofi.separator-style: solid
