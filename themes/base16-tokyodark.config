! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #151621, #151621, #11121d

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #151621, #abb2bf, #151621, #151621, #2c2d38
rofi.color-active:  #151621, #95c561, #151621, #151621, #95c561
rofi.color-urgent:  #151621, #a485dd, #151621, #151621, #a485dd

! Set the desired separator style
rofi.separator-style: solid
