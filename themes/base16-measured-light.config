! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f9f5f1, #f9f5f1, #fdf9f5

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f9f5f1, #292929, #f9f5f1, #f9f5f1, #000000
rofi.color-active:  #f9f5f1, #0158ad, #f9f5f1, #f9f5f1, #0158ad
rofi.color-urgent:  #f9f5f1, #ac1f35, #f9f5f1, #f9f5f1, #ac1f35

! Set the desired separator style
rofi.separator-style: solid
