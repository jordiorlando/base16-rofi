! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2f1823, #2f1823, #131213

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2f1823, #f15c99, #2f1823, #2f1823, #632227
rofi.color-active:  #2f1823, #008080, #2f1823, #2f1823, #008080
rofi.color-urgent:  #2f1823, #b00b69, #2f1823, #2f1823, #b00b69

! Set the desired separator style
rofi.separator-style: solid
