! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #362b27, #362b27, #241d1a

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #362b27, #b8a59d, #362b27, #362b27, #dcd2ce
rofi.color-active:  #362b27, #b0a4c3, #362b27, #362b27, #b0a4c3
rofi.color-urgent:  #362b27, #f6998f, #362b27, #362b27, #f6998f

! Set the desired separator style
rofi.separator-style: solid
