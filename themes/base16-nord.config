rofi.color-active:  #3b4252, #81a1c1, #3b4252, #3b4252, #81a1c1
rofi.color-urgent:  #3b4252, #bf616a, #3b4252, #3b4252, #bf616a

! Set the desired separator style
rofi.separator-style: solid
