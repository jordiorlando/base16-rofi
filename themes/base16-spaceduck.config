! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1b1c36, #1b1c36, #16172d

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1b1c36, #ecf0c1, #1b1c36, #1b1c36, #ffffff
rofi.color-active:  #1b1c36, #7a5ccc, #1b1c36, #1b1c36, #7a5ccc
rofi.color-urgent:  #1b1c36, #e33400, #1b1c36, #1b1c36, #e33400

! Set the desired separator style
rofi.separator-style: solid
