! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #bebed2, #bebed2, #efefef

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #bebed2, #313145, #bebed2, #bebed2, #1a1a2f
rofi.color-active:  #bebed2, #471397, #bebed2, #bebed2, #471397
rofi.color-urgent:  #bebed2, #f43979, #bebed2, #bebed2, #f43979

! Set the desired separator style
rofi.separator-style: solid
