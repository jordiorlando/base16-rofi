! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2e383c, #2e383c, #272e33

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2e383c, #d3c6aa, #2e383c, #2e383c, #fdf6e3
rofi.color-active:  #2e383c, #a7c080, #2e383c, #2e383c, #a7c080
rofi.color-urgent:  #2e383c, #7fbbb3, #2e383c, #2e383c, #7fbbb3

! Set the desired separator style
rofi.separator-style: solid
