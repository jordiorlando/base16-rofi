! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #ebebeb, #ebebeb, #ffffff

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #ebebeb, #474747, #ebebeb, #ebebeb, #282828
rofi.color-active:  #ebebeb, #0054cf, #ebebeb, #ebebeb, #0054cf
rofi.color-urgent:  #ebebeb, #bf0000, #ebebeb, #ebebeb, #bf0000

! Set the desired separator style
rofi.separator-style: solid
